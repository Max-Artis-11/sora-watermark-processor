{
  "expo": {
    "name": "SoraAIWatermarkRemoverAndAdder",
    "slug": "SoraAIWatermarkRemoverAndAdder",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "soraaiwatermarkremoverandadder",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "statusBarStyle": "light-content",
      "bundleIdentifier": "com.maxplus.soraaiwatermarkremoverandadder",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#0B2048",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "androidStatusBar": {
        "barStyle": "light-content",
        "backgroundColor": "#000000"
      },
      "package": "com.maxplus.soraaiwatermarkremoverandadder"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 220,
          "resizeMode": "contain",
          "backgroundColor": "#000000",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-sqlite",
      "expo-font",
      "expo-video",
      "expo-asset"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "deepLinkScheme": "soraaiwatermarkremoverandadder",
      "router": {},
      "eas": {
        "projectId": "a0a4e62f-2e40-4548-89f1-0802bd700ed2"
      }
    }
  }
}
